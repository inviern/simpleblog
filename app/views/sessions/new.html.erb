<% provide(:title, 'Log in') %>
<h1>Sessions#new</h1>

<%= form_for :session, url: login_path do |f| %>
    <%= f.label :email %><br>
    <%= f.text_field :email %><br>
    <br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br>
    <br>
    <%= f.submit "Log in" %><br>
<% end %>
